<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Student Record</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="/stylesheets/bootstrap.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/regular.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Red+Rose:wght@300;400;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/stylesheets/bootstrap.css">
    <link rel="stylesheet" href="/stylesheets/Styles.css">

</head>

<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="/">SDBM</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
            aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto">
                <%if(!currentUser){%>
                <li class="nav-item ">
                    <a class="nav-link" href="/">Login</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/register">Register</a>
                </li>
                <%} else {%>
                <li class="nav-item">
                    <a class="nav-link" href="#">Welcome, <%= currentUser.username %> !</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link disabled" href="logout">Logout</a>
                </li>
            </ul>
            <%}%>


        </div>
    </nav>
    <div class="container">
        <% if(error && error.length > 0) {%>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
            <% } %>
            <% if(success && success.length >0) {%>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
            <% } %>
        </div>


        <div class="container">
            <div class="tablemod">
                <table id="studentprofile" class="table">

                    <thead class="thead-dark">
                        <tr>
                            <!-- <th scope="col">NUM</th> -->
                            <th scope="col">NAME</th>
                            <th scope="col">DOB</th>
                            <th scope="col">EMAIL</th>
                            <th scope="col">PHONE</th>
                            <th scope="col">CLASS</th>
                            <th scope="col">INCHARGE</th>
                            <th scope="col">ADDRESS</th>
                            <th scope="col">M1</th>
                            <th scope="col">M2</th>
                            <th scope="col">M3</th>
                            <th scope="col">M4</th>
                            <th scope="col">M5</th>
                            <th scope="col">M6</th>
                            <th class="actbutton" scope="col"></th>
                            <th class="actbutton" scope="col"></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <!-- <th scope="row"><%= student.NUM %></th> -->
                            <td scope="row"> <%= student.NAME %> </td>
                            <td scope="row"><%= student.DOB %></td>
                            <td scope="row"><%= student.EMAIL %></td>
                            <td scope="row"><%= student.PHONE %></td>
                            <td scope="row"><%= student.CLASS %></td>
                            <td scope="row"><%= student.INCHARGE %></td>
                            <td scope="row"><%= student.ADDRESS %></td>
                            <td scope="row"><%= student.M1 %></td>
                            <td scope="row"><%= student.M2 %></td>
                            <td scope="row"><%= student.M3 %></td>
                            <td scope="row"><%= student.M4 %></td>
                            <td scope="row"><%= student.M5 %></td>
                            <td scope="row"><%= student.M6 %></td>
                            <td scope="row"><button class="btn btn-warning" id="stupdf">Pdf</button></td>
                            <td scope="row"><button class="btn btn-info" id="stucsv">CSV</button></td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <%- include('partials/footer'); -%>